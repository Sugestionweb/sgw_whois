<?xml version="1.0" ?>
<odoo>
  <!-- Logs Whois Tree View -->
  <record model="ir.ui.view" id="view_sgw_whois_query_tree">
    <field name="name">sgw.whoisquery.tree</field>
    <field name="model">sgw.whoisquery</field>
    <field name="arch" type="xml">
      <tree string="sgw_whoisquery">
        <field name="date_query" />
        <field name="sld" />
        <field name="tld" />
        <field name="is_taken" />
      </tree>
    </field>
  </record>

  <!-- Tlds Tree View -->
  <record model="ir.ui.view" id="view_whoisg_tld">
    <field name="name">sgw.whoisg_tld.tree</field>
    <field name="model">sgw.whoisg_tld</field>
    <field name="arch" type="xml">
      <tree string="sgw_whoisg_tld">
        <field name="gtld" />
        <field name="whois_server" />
        <field name="notes" />
      </tree>
    </field>
  </record>

<!-- Whois Servers Tree View -->
  <record model="ir.ui.view" id="view_whoisserver">
    <field name="name">sgw.whoisserver.tree</field>
    <field name="model">sgw.whoisserver</field>
    <field name="arch" type="xml">
      <tree string="sgw_whoisserver">
        <field name="whois_server" />
      </tree>
    </field>
  </record>


</odoo>
